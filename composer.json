{
    "name": "yieldstudio/tailwind-merge-php",
    "autoload": {
        "files": [
            "src/helpers.php"
        ],
        "psr-4": {
            "YieldStudio\\TailwindMerge\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "YieldStudio\\TailwindMerge\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "James Hemery",
            "email": "james@yieldstudio.fr"
        }
    ],
    "require": {
        "illuminate/collections": "^10.4"
    },
    "require-dev": {
        "pestphp/pest": "^2.2",
        "illuminate/support": "^10.4"
    },
    "suggest": {
        "illuminate/support": "Required to use with Laravel."
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "YieldStudio\\TailwindMerge\\Laravel\\TailwindMergeServiceProvider"
            ]
        }
    }
}
